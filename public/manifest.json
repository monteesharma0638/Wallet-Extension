{
  "name": "Kainet Wallet",
  "description": "Kainet Wallet good for you",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Kainet Wallet",
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "128": "logo192.png"
    }
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "storage"
  ]
}
